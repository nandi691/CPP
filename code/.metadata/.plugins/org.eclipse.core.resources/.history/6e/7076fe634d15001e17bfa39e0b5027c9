#include <iostream>

template <typename T, int size>
void Sort(T(&arr)[size]){
	for(int i =0; i<size-1;++i){
		for(int j =0; j<size-1;++j)
		{
			if(arr[j] > arr[j+1]){
				T temp = std::move(arr[j]);
				arr[j] = std::move(arr[j+1]);
				arr[j+1] = std::move(temp);
			}
		}
	}
}
int main()
{
	int arr[] {12,5,0,1,2};
	for (auto x:arr){
		std::cout<<x<<" " ;
	}
	std::cout<<std::endl ;
}
