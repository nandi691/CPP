#include <iostream>

//returns r value
int Add(int x, int y){
	return x+y;
}

//Returns LValue
int & Transform(int &x){
	x *= x;
	return x;
}

int main(){
	//LValues - x,y,z
	//RValues - 5,10,8
	int x = 5;
	int y = 10;
	int z = 8;

	//Expression returns R-Value
	int result = (x+y)*z;

	//Expression returns L-value
	++x = 6;
	std::cout<<x<<std::endl;
	return 0;
}
