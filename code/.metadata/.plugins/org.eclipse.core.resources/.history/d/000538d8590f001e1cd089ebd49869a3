#include "Integer.h"
#include <iostream>

/*
 * 	Integer();
	Integer(int value);
	~Integer();

	int GetValue() const;
	void SetValue(int val);
 */

Integer::Integer(){
	m_pInt = new int(0);
}

Integer::Integer(int value){
	m_pInt = new int(value);
}

Integer::~Integer(){
	delete m_pInt;
}

int Integer::GetValue() const{
	return *m_pInt;
}
